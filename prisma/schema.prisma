generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model datamunas {
  id                 Int                 @id @default(autoincrement())
  asal_kota          datamunas_asal_kota
  created_at         DateTime            @default(now())
  jam_kedatangan     String
  jumlah_rombongan   Int
  lokasi_menginap    String
  nama_ajudan        String
  nama_walikota      String
  nomor_handphone    String
  tanggal_kedatangan DateTime
  tanggal_kepulangan DateTime
  updated_at         DateTime @default(now())
  transportasi            String
  ukuran_baju_ibuk   String
  ukuran_baju_bapak  String
}

model news {
  id       Int      @id @default(autoincrement())
  tittle   String
  location String
  date     DateTime
  body     String   @db.Text
  images   String
}

model optd {
  id                 Int            @id @default(autoincrement())
  instansi           String
  jabatan            String
  nama_unit_kerja    String
  nama_ajudan        String
  nomor_handphone    String
  tanggal_kedatangan DateTime
  tanggal_kepulangan DateTime
  lokasi_menginap    String
  jumlah_rombongan   Int
  transportasi            String
  created_at         DateTime       @default(now())
  updated_at         DateTime @default(now())
  asal_kota          optd_asal_kota
  jam_kedatangan     String
  nama               String
  ukuran_baju_ibuk   String
  ukuran_baju_bapak  String
  EventParticipation String
}

model photo {
  id         Int             @id @default(autoincrement())
  url        String
  photoType  photo_photoType
  created_at DateTime        @default(now())
  updated_at DateTime
}

model user {
  id       String    @id
  name     String
  password String
  role     user_role @default(user)
}

